<GUI>
	<Icon name='I' file='XMLBuilder.ico'/>
	<Class name='C' icon='$self->{I}'/>
	<Window name='W'
		dim='100, 100, 250, 90'
		title='Listbox - example'
		class='$self->{C}'
	>
		<Listbox dim='0, 0, 100, $self->{W}->ScaleHeight'>
			<Item text='Item 1'/>
			<Item text='Item 2'/>
			<Item text='Item 3'/>
			<Item text='Item 4' default='1'/>
		</Listbox>
		<Combobox dim='120, 0, 100, 80' dropdown='1'>
			<Item text='Item 1'/>
			<Item text='Item 2'/>
			<Item text='Item 3'/>
			<Item text='Item 4' default='1'/>
			<Item text='Item 5'/>
			<Item text='Item 6'/>
			<Item text='Item 7'/>
			<Item text='Item 8'/>
			<Item text='Item 9'/>
			<Item text='Item 10'/>
		</Combobox>
	</Window>
</GUI>
