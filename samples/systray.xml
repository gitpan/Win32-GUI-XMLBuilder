<GUI>
	<Icon name='I' file='XMLBuilder.ico'/>
	<Class name='C' icon='$self->{I}'/>
	<Window name='W'
		dim='100, 100, 400, 150'
		title='System Tray Example'
		onMinimize='sub { $_[0]->Disable; $_[0]->Hide; return 1; }'
		class='$self->{C}'
	>
	<NotifyIcon name='NI'
		id='1'
		icon='$self->{I}'
		tip='System Tray Example'
		onClick='sub { $self->{W}->Enable; $self->{W}->Show; return 1; }'
	/>
	</Window>
</GUI>
